.row-fluid
  .span12.box.bordered-box.blue-border
    .box-header.blue-background
      .title
        %i.icon-pencil
        Report Job ID #{@report_job.id}
        = link_to "Run As Review", report_job_runs_path(@report_job, review_type: 'review'), method: :post, class: "btn btn-info"
        = link_to "Run Final", report_job_runs_path(@report_job, review_type: 'final'), method: :post, class: "btn btn-danger"
    .box-content
      = render partial: "report_jobs/partials/form"
